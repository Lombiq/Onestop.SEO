@model Onestop.Seo.Models.SeoPart

@{
    var description = Model.DescriptionOverride;
    var htmlAttributes = new { @class = "text large" };

    if (String.IsNullOrEmpty(description)) {
        description = Model.GeneratedDescription;
        htmlAttributes = new { @class = "text large onestop-seo-generated-default" };
    }
}
@Html.TextBox("DescriptionOverride", description, htmlAttributes )